 ;; If you edit it by hand, you could mess it up, so be careful.
 ;; Your init file should contain only one such instance.
 ;; If there is more than one, they won't work right.
 '(default ((t (:inherit nil :stipple nil :inverse-video nil :box nil :strike-through nil :overline nil :underline nil :slant normal :weight normal :height 113 :width normal :foundry "ADBO" :family "Source Code Pro")))))


;; set up unicode
(prefer-coding-system       'utf-8)
(set-default-coding-systems 'utf-8)
(set-terminal-coding-system 'utf-8)
(set-keyboard-coding-system 'utf-8)


;;spaces over tabs
(setq-default indent-tabs-mode nil)
(setq tab-width 4)
(setq-default c-basic-offset 4)

;;backup scenes
(setq
   backup-by-copying t      ; don't clobber symlinks
   backup-directory-alist
    '(("." . "~/.saves"))    ; don't litter my fs tree
   delete-old-versions t
   kept-new-versions 6
   kept-old-versions 2
   version-control t)       ; use versioned backups


;;auto-complete
(require `auto-complete)
;;default config
(require `auto-complete-config)
(ac-config-default)


;;yas
(add-to-list 'load-path
              "~/.emacs.d/plugins/yasnippet")
(require `yasnippet)
(yas-global-mode 1)
;;dont run yas in term
(add-hook 'term-mode-hook (lambda()
                (yas-minor-mode -1)))




;;flycheck
(add-hook 'after-init-hook #'global-flycheck-mode)
(setq-default flycheck-disabled-checkers '(c/c++-clang))

;;anaconda mode
(add-hook 'python-mode-hook 'anaconda-mode)
;;eyebrowse
(eyebrowse-mode t)
;;python-3.5
(setq python-shell-interpreter "python3")

;;cedet
(semantic-mode 1)
(defun semanticadd()
  (add-to-list `ac-sources `ac-source-semantic))
(add-hook `c-mode-common-hook `semanticadd)
(global-ede-mode 1)


